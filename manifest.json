{

  "name": "switch",

  "version": "0.0.1",

  "manifest_version": 2,

  "default_locale": "en",

  "devtools_page": "devtools.html",

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/**",
    "http://*/**"
  ]

}
